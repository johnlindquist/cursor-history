{
  "name": "cursor-history",
  "description": "A new CLI generated with oclif",
  "version": "0.0.0",
  "author": "John Lindquist",
  "bin": {
    "chi": "./bin/run.js"
  },
  "bugs": "https://github.com/johnlindquist/cursor-history/issues",
  "dependencies": {
    "@inquirer/search": "^3.0.9",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "better-sqlite3": "^11.8.1",
    "clipboardy": "^4.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/johnlindquist/cursor-history",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "chi",
    "dirname": "chi",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      },
      "search": {
        "description": "Search and view conversations"
      }
    },
    "default": "search"
  },
  "repository": "johnlindquist/cursor-history",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "pnpm eslint . --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
