{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    ["@semantic-release/github", {
      "assets": [
        {"path": "dist/index.js", "label": "Main JavaScript file", "name": "index.js"},
        {"path": "dist/index.d.ts", "label": "Main TypeScript Declaration", "name": "index.d.ts"},
        {"path": "dist/commands/**/*.js", "label": "Command JavaScript files"},
        {"path": "dist/commands/**/*.d.ts", "label": "Command TypeScript Declarations"},
        {"path": "dist/utils/**/*.js", "label": "Utility JavaScript files"},
        {"path": "dist/utils/**/*.d.ts", "label": "Utility TypeScript Declarations"}
      ],
      "successComment": false,
      "failComment": false,
      "failTitle": false,
      "releasedLabels": false
    }]
  ]
} 